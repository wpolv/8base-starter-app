{
  "parser": "babel-eslint",
  "plugins": [
    "prettier"
  ],
  "extends": [
    "plugin:prettier/recommended",
    "plugin:import/recommended",
    "react-app"
  ],
  "rules": {
    "prettier/prettier": [
	"error",
	  {
        "endOfLine": "auto"
      }
	],
    "no-console": ["warn"],
    "no-trailing-spaces": "warn",
    "quote-props": ["error", "as-needed"]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  },
  
}
